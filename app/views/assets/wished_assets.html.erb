<h3>My wishlist</h3>
<hr>
<div class="row">
  <% if @wished_assets.any? %>
    <% @wished_assets.each do |wished_asset|  %>
      <%= render partial: 'assets/asset', locals: { asset: Asset.find(wished_asset.asset_id) } %>
    <% end %>
    <% else %>
    <h4>Your wishlist is empty.</h4>
  <% end %>
</div>